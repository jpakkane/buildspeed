<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN"
"http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node name="/network">
  <interface name="com.example.network">
    <method name="get_modems">
      <arg direction="out" type="as" />
    </method>
    <method name="unlock_sim">
      <arg direction="in"  type="i" name="value" />
    </method>
    <method name="set_bt_killswitch">
      <arg direction="in"  type="i" name="value" />
    </method>
    <method name="connect_wlan">
      <arg direction="in"  type="i" name="value" />
    </method>
    <method name="set_flightmode">
      <arg direction="in"  type="i" name="value" />
    </method>
    <method name="get_wlans">
      <arg direction="out" type="as" />
    </method>
    <method name="set_wlan_killswitch">
      <arg direction="in"  type="i" name="value" />
    </method>
  </interface>
  <interface name="org.freedesktop.DBus.Introspectable">
    <method name="Introspect">
      <arg direction="out" type="s" />
    </method>
  </interface>
</node>

